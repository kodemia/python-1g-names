Algoritmo GANANCIAS
	// UNA PERSONA QUE DESEA INVERTIR SU CAPITAL EN UN BANCO Y DESEA SABER CUÁNTO GANARÁ DESPUÉS DE UN AÑO, SI EL BANCO LE PAGA A RAZÓN DE 2 POR CIENTO MENSUAL.
	Definir capital, ano como entero;
	Definir ganancia, inte como real;
	INTE <- 0.02;
	ANO <- 12;
	Escribir "Cuanto dinero quiere invertir";
	Leer capital;
	ganancia <- (capital * inte);
	Escribir "Tu ganancia será de: ",ganancia;
FinAlgoritmo
